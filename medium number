#include <iostream>
#include <algorithm>

int main() {
    int t;
    std::cin >> t; // Read the number of test cases

    while (t--) {
        int a, b, c;
        std::cin >> a >> b >> c; // Read the three distinct integers

        // Store the numbers in an array
        int numbers[3] = {a, b, c};

        // Sort the array
        std::sort(numbers, numbers + 3);

        // The median is the second element in the sorted array
        std::cout << numbers[1] << std::endl;
    }

    return 0;
}
